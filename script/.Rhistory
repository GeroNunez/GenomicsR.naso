# Define el valor mínimo y el máximo
lower=1; upper=5
# Crea una variable squaresum que tenga al inicio 0
squaresum = 0
for (ii in lower:upper)
{
cat(ii, ii^2, "\n")
squaresum = squaresum + ii^2
}
cat ("the sum of it all is", squaresum)
